#registrador 1 recebe todos os valores que est√£o na memoria. Um de cada vez a cada ciclo do loop
PegaDado:
	ldw r1,0(r2)
	addi r2,r2,4
	addi r3,r3,1
	bne r3,r4,PegaDado
